{"semicolon": [ true, "never" ],
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-airbnb"
    ],
    "jsRules": {},
    "rules": {
        "no-consecutive-blank-lines": [true, 2],
        "semicolon": [true, "never"],
        "import-name": [ false ],
        "max-line-length": [ false ],
        "ordered-imports": false,
        "interface-name": false,
        "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}